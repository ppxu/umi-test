{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rules": {
    "interface-over-type-literal": false,
    "interface-name": [true, "never-prefix"],
    "jsx-no-lambda": false,
    "member-access": false,
    "member-ordering": false,
    "no-console": false,
    "no-empty": false,
    "no-empty-interface": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false
  },
}
